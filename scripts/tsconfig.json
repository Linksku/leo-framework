{
  "extends": "../tsconfig-base.json",
  "compilerOptions": {
    "checkJs": true,
    "noImplicitAny": false,
    "rootDir": "..",
    "types": [
      "webpack-env",
    ],
  },
  "include": [
    "../*.ts",
    "../**/.*.ts",
    "../**/*.js",
    "../**/.*.js",
    "../**/*.cjs",
    "../**/.*.cjs",
    "../**/*.mjs",
    "../**/.*.mjs",
  ],
}
