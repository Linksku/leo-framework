<!doctype html>
<html lang="en">
<head>
  <title><%- title %></title>
  <meta name="viewport" content="width=device-width,minimum-scale=1,initial-scale=1,maximum-scale=5,user-scalable=0">

  <script defer src="<%= assetsUrl %>/js/main<%= fileVersion %>.js" charset="utf-8" fetchpriority="high"></script>
  <link rel="preload" href="<%= assetsUrl %>/js/chunks/deferred<%= fileVersion %>.js" as="script" fetchpriority="low">

  <style>html{background-color:#fafafa}body{display:none}</style>
  <link rel="stylesheet" href="<%= assetsUrl %>/css/main<%= fileVersion %>.css" media="none" onload="if(media!='all')media='all'">
  <link rel="stylesheet" href="<%= assetsUrl %>/css/chunks/deferred<%= fileVersion %>.css" media="none" onload="if(media!='all')media='all'">
  <link rel="preload" href="https://fonts.gstatic.com/s/roboto/v20/KFOmCnqEu92Fr1Mu4mxK.woff2" as="font" type="font/woff2" crossorigin fetchpriority="low">
  <link rel="preload" href="https://fonts.gstatic.com/s/roboto/v20/KFOlCnqEu92Fr1MmEU9fBBc4.woff2" as="font" type="font/woff2" crossorigin fetchpriority="low">

<% if (production) { %>
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <meta name="description" content="<%= description %>">
  <link rel="icon" sizes="32x32" href="<%= assetsUrl %>/icon32.png">
  <link rel="shortcut icon" href="<%= basePath %>/favicon.ico">
  <meta name="theme-color" content="#ffffff">
  <meta name="mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="application-name" content="<%= name %>">
  <meta name="apple-mobile-web-app-title" content="<%= name %>">
  <link rel="apple-touch-icon" sizes="180x180" href="<%= assetsUrl %>/icon180.png">
  <link rel="icon" sizes="192x192" href="<%= assetsUrl %>/icon192.png">
  <link rel="manifest" href="<%= basePath %>/manifest.json">
<% } %>
</head>
<body>
  <%- content %>
</body>
</html>
