<title><%- title %></title>
<meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=2,user-scalable=0,viewport-fit=cover">

<meta name="description" content="<%= description %>">
<meta name="theme-color" content="#ffffff">
<meta name="mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="application-name" content="<%= name %>">
<meta name="apple-mobile-web-app-title" content="<%= name %>">
<link rel="manifest" href="<%= basePath %>/manifest.json">

<link rel="icon" sizes="32x32" href="<%= assetsUrl %>/icon32.png">
<link rel="shortcut icon" href="<%= basePath %>/favicon.ico">
<link rel="apple-touch-icon" sizes="180x180" href="<%= assetsUrl %>/ios-icon180.png">
<link rel="icon" sizes="192x192" href="<%= assetsUrl %>/android-icon192.png">
